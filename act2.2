#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    string input;
    getline(cin, input);

    vector<string> substrings;
    int length = input.length();

 
    for (int i = 0; i < length; ++i) {
        for (int j = 1; j <= length - i; ++j) {
            substrings.push_back(input.substr(i, j));
        }
    }

    
    sort(substrings.begin(), substrings.end());

    
    for (const auto& s : substrings) {
        cout << s.length() << endl;
    }

    return 0;
}

